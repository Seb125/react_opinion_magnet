{
  "name": "react-opinion-magnet",
  "version": "0.0.14",
  "description": "Our component submit customer feedback, assesses comments and reviews, and empowers unicorn companies to take responsive actions ",
  "license": "MIT",
  "author": "Zeynep Sariteke,Sebastian Schwarz",
  "repository": "github:zsaritek/react-opinion-magnet",
  "keywords": [
    "feedback",
    "review",
    "opinion",
    "react",
    "component",
    "analysis"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/react-opinion-magnet.umd.js",
  "module": "./dist/react-opinion-magnet.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-opinion-magnet.es.js",
      "require": "./dist/react-opinion-magnet.umd.js",
      "styles": "./dist/style.css"
    },
    "./css": "./dist/style.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "c8": "^9.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "vite": "^5.0.11",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react-simple-star-rating": "^5.1.7"
  }
}
